{
  "name": "@iungo/redux",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf lib/* es6/* .cache",
    "build": "npm run clean && npm run build:tsc:es5 && npm run build:tsc:es6",
    "build:tsc:es5": "tsc",
    "build:tsc:es6": "tsc --project tsconfig.es6.json",
    "start": "npm-run-all -p start:tsc:es5 start:tsc:es6",
    "start:tsc:es5": "npm run build:tsc:es5 -- --watch --preserveWatchOutput",
    "start:tsc:es6": "npm run build:tsc:es6 -- --watch --preserveWatchOutput"
  },
  "devDependencies": {
    "@iungo/sdk": "*",
    "@iungo/sdk-utils": "*",
    "@types/node": "^18.8.5",
    "@types/react": "^16.14.21",
    "@types/react-redux": "^6.0.9",
    "@types/redux": "^3.6.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.14.0",
    "react-redux": "^5.1.2",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@iungo/sdk": ">=4",
    "react": "^16.0.0",
    "redux": "*"
  },
  "main": "./lib/index.js",
  "module": "./es6/index.js",
  "types": "./lib/index.d.ts",
  "author": {
    "name": "Iungo, Inc.",
    "url": "https://developers.iungo.com"
  },
  "contributors": [
    {
      "name": "Daniel Paixao"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/iungopbx/iungo-js.git"
  },
  "bugs": {
    "url": "https://github.com/iungopbx/iungo-js/issues"
  },
  "homepage": "https://github.com/iungopbx/iungo-js",
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
